{
  "name": "redux-di",
  "version": "1.0.7",
  "description": "Redux thunk middleware with dependency injection",
  "keywords": [
    "dependency injection",
    "di",
    "redux",
    "action creator",
    "laat"
  ],
  "license": "MIT",
  "author": {
    "name": "Sigurd Fosseng",
    "email": "sigurd@fosseng.net",
    "url": "https://github.com/laat"
  },
  "files": [
    "lib",
    "*.js"
  ],
  "main": "lib/index.js",
  "repository": "laat/redux-di",
  "scripts": {
    "build:js": "babel -d lib --ignore *test.js src",
    "build": "npm-run-all -p build:*",
    "lint": "eslint src",
    "test:jest": "jest",
    "test": "npm-run-all -p lint test:*",
    "prebuild": "rimraf lib",
    "precommit": "lint-staged",
    "prepublish": "npm run build"
  },
  "lint-staged": {
    "*.{js,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.7.2",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.2.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^4.2.3",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.7.0",
    "rimraf": "^2.6.1",
    "sinon": "^4.0.2"
  },
  "engines": {
    "node": ">=4"
  }
}
