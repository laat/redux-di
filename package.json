{
  "name": "redux-di",
  "version": "1.0.1",
  "description": "Redux thunk middleware with dependency injection",
  "keywords": [
    "dependency injection",
    "di",
    "redux",
    "action creator",
    "laat"
  ],
  "license": "MIT",
  "author": {
    "name": "Sigurd Fosseng",
    "email": "sigurd@fosseng.net",
    "url": "https://github.com/laat"
  },
  "files": [
    "lib",
    "*.js"
  ],
  "main": "lib/index.js",
  "repository": "laat/redux-di",
  "scripts": {
    "build:js": "babel -d lib --ignore *test.js src",
    "build:flow": "flow-copy-source -v -i '*test.js' src lib",
    "build": "npm-run-all -p build:*",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "eslint src",
    "test:tape": "tape -r babel-register 'src/**/*test.js'",
    "test": "npm-run-all lint flow test:*",
    "prebuild": "rimraf lib",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.8.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb-base": "^7.0.0",
    "eslint-plugin-import": "^1.8.0",
    "flow-bin": "^0.32.0",
    "flow-copy-source": "^1.1.0",
    "npm-run-all": "^3.1.0",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.4",
    "tape": "^4.5.1"
  },
  "engines": {
    "node": ">=4"
  }
}
